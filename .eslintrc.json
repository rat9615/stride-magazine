{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "plugins": ["prettier"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "linebreak-style": ["error", "windows"]
    }
  }